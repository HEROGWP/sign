<h1>回饋</h1>

<div class="row">
  <%= form_for(@feedback) do |f| %>
      <%= render "shared/form_errors", record: @feedback%>

      <div class="field">
        <%= f.label "問題1" %><br>
        <%= f.text_field :q1 %>
      </div>

      <div class="field">
        <%= f.label "問題2" %><br>
        <%= f.text_field :q2 %>
      </div>
      
      <div class="field">
        <%= f.label "問題3" %><br>
        <%= f.text_field :q3 %>
      </div>
      
      <div class="field">
        <%= f.label "問題4" %><br>
        <%= f.text_field :q4 %>
      </div>

      <div class="field">
        <%= f.label "問題5" %><br>
        <%= f.text_field :q5 %>
      </div>
 
      <div class="actions">
        <%= f.submit "確認", class: "btn btn-primary" %>
      </div>
  <% end %>
</div>

